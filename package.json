{
  "name": "cte-commander",
  "version": "0.0.1",
  "description": "Client(Commander) Application for Capture the Ether(https://capturetheether.com/).",
  "main": "index.js",
  "author": "foxytanuki <biz@foxytanuki.dev> (https://github.com/foxytanuki)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "yarn hardhat compile",
    "clean": "yarn hardhat clean",
    "generate-types": "typechain --target=ethers-v5 'abi/*.json'",
    "postinstall": "yarn generate-types",
    "typecheck": "tsc --noEmit",
    "start": "yarn docker:run yarn ts-node",
    "start:h": "yarn docker:run yarn hardhat run",
    "docker:run": "docker-compose run -u root cte-commander"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/bn.js": "^5.1.0",
    "@types/node": "^16.6.1",
    "ethers": "^5.0.31",
    "hardhat": "^2.6.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "typechain": "^5.1.2",
    "typescript": "4.3.5"
  },
  "dependencies": {}
}
