{
  "name": "cte-commander",
  "version": "0.0.1",
  "description": "Client(Commander) Application for Capture the Ether(https://capturetheether.com/).",
  "main": "index.js",
  "author": "Yuta Tsurusaki <biz@yuta-tsurusaki.com> (https://github.com/yuta-tsurusaki)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "yarn hardhat compile",
    "clean": "yarn hardhat clean",
    "generate-types": "typechain --target=ethers-v5 'abi/*.json'",
    "postinstall": "yarn generate-types",
    "typecheck": "tsc --noEmit",
    "start": "docker-compose run cte-commander yarn ts-node"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^6.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/bn.js": "^5.1.0",
    "@types/node": "^16.6.1",
    "ts-node": "^9.1.1",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "ethers": "^5.0.31",
    "hardhat": "^2.6.0",
    "typechain": "^4.0.2",
    "typescript": "4.2.2"
  },
  "dependencies": {
  }
}
